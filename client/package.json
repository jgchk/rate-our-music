{
  "name": "our-elm-spa-app",
  "version": "1.0.0",
  "description": "A project created with elm-spa",
  "scripts": {
    "start": "run-s build:dev dev",
    "build": "run-s build:elm-spa build:elm build:compress build:mangle",
    "build:elm": "elm make src/Main.elm --optimize --output=public/dist/elm.compiled.js",
    "build:compress": "terser public/dist/elm.compiled.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters=true,keep_fargs=false,unsafe_comps=true,unsafe=true,passes=4' --output=public/dist/elm.compiled.js",
    "build:mangle": "terser public/dist/elm.compiled.js --mangle --output=public/dist/elm.compiled.js",
    "build:elm-spa": "elm-spa build .",
    "build:dev": "run-s build:elm-spa build:dev:elm",
    "build:dev:elm": "elm make src/Main.elm --debug --output=public/dist/elm.compiled.js",
    "dev": "run-p dev:elm-spa dev:elm",
    "dev:elm": "elm-live src/Main.elm -u -d public -x /graphql -y http://localhost:3030/graphql -- --debug --output=public/dist/elm.compiled.js",
    "dev:elm-spa": "chokidar src/Pages -c \"elm-spa build .\"",
    "api": "elm-graphql http://127.0.0.1:3030/graphql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "elm": "^0.19.1-5",
    "elm-live": "^4.0.2",
    "elm-spa": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "terser": "^5.6.0"
  }
}
